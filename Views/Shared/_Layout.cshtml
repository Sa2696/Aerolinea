    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="FOOTER">

        <div class="iconos">
            <br>
            <img src="~/img/icon-facebook.jpg">
            <img src="~/img/icon-googleplus.jpg">
            <img src="~/img/icon-pinterest.jpg">
            <img src="~/img/icon-twitter.jpg">
            <br>
            <label style="color: #ffffff;" class="text-center"> © 2019 Aerolinea S&C - Todos Los Derechos Reservados</label>
        </div>
    </footer>
   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
